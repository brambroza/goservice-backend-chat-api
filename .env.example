PORT=8082
NODE_ENV=development
LOG_LEVEL=info

# LINE credentials
LINE_CHANNEL_SECRET=replace-me
LINE_CHANNEL_ACCESS_TOKEN=replace-me

# Facebook credentials
FB_APP_SECRET=replace-me
FB_PAGE_ACCESS_TOKEN=replace-me
FB_VERIFY_TOKEN=verify-token

# Third-party webhook signing secret
THIRD_PARTY_WEBHOOK_SECRET=replace-me

# Kafka
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=chat-api
